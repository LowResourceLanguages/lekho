all: Makefile $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	test -d $(HOME)/Lekho/bin/ || mkdir -p $(HOME)/Lekho/bin/
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: Gui.pro  
	$(QMAKE) Gui.pro
qmake: 
	@$(QMAKE) Gui.pro

dist: 
	@mkdir -p $(HOME)/Lekho/obj/Gui && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) $(HOME)/Lekho/obj/Gui/ && ( cd `dirname $(HOME)/Lekho/obj/Gui` && $(TAR) Gui.tar Gui && $(GZIP) Gui.tar ) && mv `dirname $(HOME)/Lekho/obj/Gui`/Gui.tar.gz . && rm -rf $(HOME)/Lekho/obj/Gui

mocclean:
