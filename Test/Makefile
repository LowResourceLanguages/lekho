####### Implicit rules

.SUFFIXES: .c .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	test -d ../bin/ || mkdir -p ../bin/
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: DocumentTest.pro  
	$(QMAKE) DocumentTest.pro
qmake: 
	@$(QMAKE) DocumentTest.pro

dist: 
	@mkdir -p ../Obj/DocumentTest && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) ../Obj/DocumentTest/ && ( cd `dirname ../Obj/DocumentTest` && $(TAR) DocumentTest.tar DocumentTest && $(GZIP) DocumentTest.tar ) && mv `dirname ../Obj/DocumentTest`/DocumentTest.tar.gz . && rm -rf ../Obj/DocumentTest

mocclean:

uiclean:

clean:
	-rm -f $(OBJECTS) 
	-rm -f *~ core *.core


####### Sub-libraries

distclean: clean
	-rm -f ../bin/$(TARGET) $(TARGET)
	

